{
  "name": "learn-angular-change-detaction",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "start-dev": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ng build --output-path angularapp --aot --prod",
    "execute:hooks-folder": "git config core.hooksPath .githooks",
    "execute:prepare-commit-msg": "shx chmod +x .githooks/prepare-commit-msg",
    "execute:pre-commit": "shx chmod +x .githooks/pre-commit",
    "init-hooks": "npm run execute:pre-commit && npm run execute:prepare-commit-msg && npm run execute:hooks-folder",
    "rimraf": "rimraf",
    "precommit": "./node_modules/.bin/lint-staged",
    "postcommit": "git update-index --again",
    "prettier:all": "prettier --write src/**/*.ts && prettier --write src/**/*.js",
    "clean": "npm cache clean -f && rm -rf -- node_modules doc coverage dist compiled dll",
    "clean:dist": "npm run rimraf -- dist",
    "clean:compiled": "npm run rimraf -- compiled"
  },
